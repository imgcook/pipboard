{
  "name": "@pipcook/pipboard",
  "version": "3.0.0",
  "description": "board for pipcook",
  "scripts": {
    "start": "npm run prebuild && parcel src/index.html",
    "proxyassets": "node scripts/server.js",
    "prebuild": "rimraf dist/ && rimraf .cache/ && rimraf .parcel-cache/",
    "build": "parcel build src/index.html --dist-dir dist --no-scope-hoist",
    "postbuild": "node ./scripts/injectAssets.js",
    "lint": "npm run test:eslint && npm run test:stylelint",
    "test:eslint": "eslint src/ --ext .js --ext .jsx",
    "test:stylelint": "stylelint src/**/*.{css,less}",
    "clean": "rimraf dist/ && rimraf .cache/ && rimraf .parcel-cache/ && rimraf node_modules/",
    "analyze": "npm run prebuild && PARCEL_BUNDLE_ANALYZER=1 parcel build src/index.html --dist-dir dist --no-source-maps",
    "precommit": "lint-staged"
  },
  "alias": {
    "src": "./src/"
  },
  "browserslist": [
    "> 0.2%",
    "not dead"
  ],
  "lint-staged": {
    "*.{js,jsx,json,scss,less,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@antv/g2": "^4.1.6",
    "@pipcook/pipcook-core": "^1.1.2",
    "@pipcook/sdk": "^1.2.1-6fd0361-beta.0",
    "@tensorflow/tfjs": "1.7.0",
    "antd": "^4.8.5",
    "axios": "^0.18.1",
    "codemirror": "^5.52.2",
    "dayjs": "^1.9.6",
    "highlight.js": "^10.5.0",
    "jimp": "^0.10.0",
    "less": "^3.12.2",
    "moment": "^2.22.1",
    "path": "^0.12.7",
    "react": "^16.0.0",
    "react-canvas-draw": "^1.1.0",
    "react-codemirror2": "^7.1.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.1",
    "download": "^8.0.0",
    "eslint": "^7.15.0",
    "eslint-plugin-react": "^7.21.5",
    "express": "^4.17.1",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "parcel": "^2.0.0-beta.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^4.1.2"
  }
}
